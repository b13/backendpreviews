{
  "name": "b13/backendpreviews",
  "type": "typo3-cms-extension",
  "description": "Enhanced Fluid based backend element previews",
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/Web",
      "extension-key": "backendpreviews"
    }
  },
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^7.4 || ~8.0",
    "typo3/cms-backend": "^10.4 || ^11.5 || ^12.4 || ^13.4 || ^14.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "typo3/coding-standards": "^0.5.5"
  },
  "suggest": {
    "b13/listelements": "*"
  },
  "autoload": {
    "psr-4": {
      "B13\\Backendpreviews\\": "Classes/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
